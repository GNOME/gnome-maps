using Gtk 4.0;

template $Gjs_PoiCategoryGobackRow: ListBoxRow {
  accessibility {
    labelled-by: [
      label,
    ];
  }

  margin-top: 6;
  margin-start: 6;
  margin-end: 6;

  child: Box {
    spacing: 6;

    Image {
      icon-name: "go-previous-symbolic";
      valign: center;
    }

    Label label {
      label: _("Back to Main Categories");
      xalign: 0;
    }
  };
}
