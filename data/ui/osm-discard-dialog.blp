using Gtk 4.0;
using Adw 1;

template $Gjs_OSMDiscardDialog: Adw.AlertDialog {
  heading: _("Discard Changes?");
  body: _("Your current changes to the map will be lost");
  default-response: "cancel";
  close-response: "cancel";

  responses [
    cancel: _("_Cancel"),
    discard: _("_Discard") destructive,
  ]
}
