  <bug-database
  rdf:resource="https://gitlab.gnome.org/GNOME/gnome-maps/issues" />

  <maintainer>
    <foaf:Person>
      <foaf:name>Marcus Lundblad</foaf:name>
      <foaf:mbox rdf:resource="mailto:ml@dfupdate.se" />
      <gnome:userid>mlundblad</gnome:userid>
    </foaf:Person>
  </maintainer>
</Project>
